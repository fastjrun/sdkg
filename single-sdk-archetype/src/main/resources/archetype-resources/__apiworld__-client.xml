				<method name="check" version="" path="check" method="Get"
					remark="是否有新文章检测" />
				<method name="latests" version="" path="latests"
					remark="最近版本列表">
					<response>
						<object name="body"
							class="packet.api.ArticleListResponseBody" />
					</response>
				</method>
			</service>
		</apiController>
	</apiControllers>
