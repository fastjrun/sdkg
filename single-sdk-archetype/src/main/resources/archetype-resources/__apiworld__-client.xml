	<apiControllers>
		<apiController name="UserApiController"
			path="/api/user/" clientName="UserApiClient"
			clientParent="com.fastjrun.client.BaseApiClient" remark="用户管理接口"
			tags="第三方接口">
			<service name="userService" class="UserServiceRestApi">
				<method name="register" version="" path="register"
					remark="注册">
					<request>
						<object name="body"
							class="packet.api.RegistserRestRequestBody" />
					</request>
				</method>
				<method name="login" version="" path="login" remark="登录">
					<request>
						<object name="body" class="packet.api.LoginRestRequestBody" />
					</request>
					<response>
						<object name="body"
							class="packet.api.LoginRestResponseBody" />
					</response>
				</method>
				<method name="login" version="v1_1" path="login"
					remark="登录v1.1">
					<request>
						<object name="body" class="packet.api.LoginRestRequestBody" />
